        var id = $(this).attr("tab-id");

        $(this).parents("[tab-title]").find("[tab-id]").removeClass("active");
        $(this).addClass("active");

        $("[tab-content='" + tab + "']").find("[tab-id]").removeClass("active");
        $("[tab-content='" + tab + "']").find("[tab-id='" + id + "']").addClass("active");
    });

    
});