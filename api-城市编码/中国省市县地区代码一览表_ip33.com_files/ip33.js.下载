//反射服务器方法
$.Invoke = function () {

    var api = "//api.ip33.com";

    //新版
    if (arguments.length == 3)
    {
        var url = api + arguments[0];
        var args = arguments[1];
        var fun = arguments[2];
    }
    else
    {
        var url = api + "/" + arguments[0] + "/" + arguments[1];
        var args = arguments[2];
        var fun = arguments[3];
    }

    $.ajax({
        type: "POST",
        url: url,
        data: args,
        dataType: "json",
        success: fun
    });
}


